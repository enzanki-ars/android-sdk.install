<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>android-sdk.install</id>
    <version>24.4.1</version>
    <packageSourceUrl>https://github.com/enzanki-ars/android-sdk.install</packageSourceUrl>
    <owners>enzanki_ars</owners>
	
    <title>android-sdk.install (Install)</title>
    <authors>Google Inc.</authors>
    <projectUrl>https://developer.android.com/studio/index.html</projectUrl>
    <iconUrl>https://developer.android.com/studio/images/studio-icon.png</iconUrl>
    <copyright>2009-2012 The Android Open Source Project</copyright>
    <licenseUrl>https://developer.android.com/studio/terms.html</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <docsUrl>https://developer.android.com/studio/intro/index.html/</docsUrl>
	<releaseNotes>https://developer.android.com/studio/releases/sdk-tools.html</releaseNotes>
    <mailingListUrl>https://groups.google.com/forum/#!forum/adt-dev</mailingListUrl>
    <bugTrackerUrl>http://b.android.com/</bugTrackerUrl>
	<projectSourceUrl>https://android.googlesource.com/new-password</projectSourceUrl>
    <tags>android-sdk.install admin</tags>
    <summary>Installs the Android SDK for all users.  </summary>
    <description>Installs the Android SDK for all users (`C:\Program Files\Android\android-sdk\` or `C:\Program Files (x86)\Android\android-sdk\` for 64-bit users). This package also adds the `tools` and `platform-tools` to the path.</description>
    <dependencies>
      <dependency id="jdk8" version="8.0.102" />
      <dependency id="chocolatey-uninstall.extension" version="1.1.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>
